{
    "version": 2,
    "builds": [
        {
            "src": "src/vercel-server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/vercel-server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "PORT": "3000",
        "HOST": "0.0.0.0",
        "VERCEL": "1"
    }
}